{
  "name": "solidity-skeleton",
  "version": "0.5.0",
  "description": "Smart contracts of the Genie",
  "main": "index.js",
  "scripts": {
    "test": "truffle test",
    "build": "truffle compile",
    "compile": "truffle compile",
    "flatten": "scripts/flatten.sh",
    "abi": "scripts/abi.sh",
    "deploy-dev": "truffle migrate --network development",
    "deploy-kovan": "truffle migrate --network kovan",
    "deploy-ropsten": "truffle migrate --network ropsten",
    "deploy-fuse": "truffle migrate --network fuse",
    "deploy-mainnet": "truffle migrate --network mainnet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonprou/solidity-dev.git"
  },
  "standard": {
    "env": {
      "mocha": true,
      "truffle/globals": true
    },
    "plugins": [
      "truffle"
    ]
  },
  "author": "leonprou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leonprou/solidity-dev/issues"
  },
  "homepage": "https://github.com/leonprou/solidity-dev#readme",
  "dependencies": {
    "node-jq": "^1.11.3",
    "openzeppelin-solidity": "2.2.0",
    "truffle": "^5.1.23",
    "truffle-flattener": "^1.5.0",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "devDependencies": {
    "bignumber.js": "^9.0.0",
    "eslint-plugin-truffle": "^0.3.1",
    "ethereumjs-abi": "^0.6.8",
    "truffle-assertions": "^0.9.2"
  }
}
